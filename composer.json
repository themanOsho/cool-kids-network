{
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "doctrine/instantiator": "^1.5",
    "squizlabs/php_codesniffer": "^3.11",
    "wp-coding-standards/wpcs": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "CoolKidsNetwork\\": "includes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CoolKidsNetwork\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
